<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<interface xmlns="http://giove.isti.cnr.it" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ns3="http://schemas.xmlsoap.org/wsdl/" xmlns:ns4="uri:xs" type="cui-gestural" schema-version="1.5.3">
    <data/>
    <presentation name="gestureTest">
        <dialog_model operator="Choice">
            <gestureModel>
                <bodyGesture name="moveSelection">
                    <composition iterative="true" operator="sequence">
                        <bodyPoint type="shoulderLeft">
                            <accepts>
<invoke_function name="axisParallel">
    <parameter name="point1" data_ref="body:shoulderLeft"/>
    <parameter name="point2" data_ref="body:shoulderRight"/>
    <parameter name="axis" data_ref="x"/>
</invoke_function>
                            </accepts>
                            <completed>
<script>
    <change_property property_value="body:handLeft" interactor_id="ciao" property_name="focusPoint"/>
    <raise interactor_id="button" event_name="activation"/>
</script>
                            </completed>
                        </bodyPoint>
                        <composition operator="disabling">
                            <composition operator="disabling">
<bodyPoint type="handRight" iterative="true"/>
                            </composition>
                            <bodyPoint type="shoulderLeft">
<accepts operator="not">
    <invoke_function name="axisParallel">
        <parameter name="point1" data_ref="body:shoulderLeft"/>
        <parameter name="point2" data_ref="body:shoulderRight"/>
        <parameter name="axis" data_ref="x"/>
    </invoke_function>
</accepts>
                            </bodyPoint>
                        </composition>
                    </composition>
                </bodyGesture>
            </gestureModel>
        </dialog_model>
        <grouping role="none" hierarchy="false" ordering="false" continuous_update="false" id="ciao" hidden="false">
            <grid>
                <grid_row height="40%">
                    <cell vertical_align="top" horizontal_align="left">
                        <description id="title" enabled="true" focus="false" hidden="false" continuous_update="false">
                            <text>
<string>Il mio bellissimo titolo</string>
<font_settings name="serif" color="blue" size="76pt" align="center" mark="false"/>
                            </text>
                        </description>
                    </cell>
                </grid_row>
                <grid_row>
                    <cell vertical_align="top" horizontal_align="left">
                        <description id="prova_image" enabled="true" focus="false" hidden="false" continuous_update="false">
                            <image source="C:\ciao\prova.png" horizontal_align="left" vertical_align="top">
<label label_position="down">
    <text>
        <string>Prova</string>
    </text>
    <background/>
</label>
                            </image>
                        </description>
                    </cell>
                    <cell vertical_align="top" horizontal_align="left">
                        <activator id="bottone" enabled="true" focus="false" hidden="false" continuous_update="false">
                            <button>
<label label_position="left">
    <text>
        <string>Click me</string>
        <font_settings name="serif" color="blue" size="76pt" align="center" mark="false"/>
    </text>
    <background/>
</label>
                            </button>
                        </activator>
                    </cell>
                    <cell vertical_align="top" horizontal_align="left">
                        <numerical_edit_in_range min="0.0" max="120.0" continuous="yes" id="range" enabled="true" focus="false" hidden="false" continuous_update="false">
                            <track_bar value="1" length="20">
<stop_value>13</stop_value>
                            </track_bar>
                        </numerical_edit_in_range>
                    </cell>
                    <cell vertical_align="top" horizontal_align="left"/>
                </grid_row>
            </grid>
            <events/>
        </grouping>
        <events>
            <on_render>
                <handler>
                    <script>
                        <change_property/>
                    </script>
                </handler>
            </on_render>
        </events>
        <presentation_settings>
            <title></title>
            <background/>
            <font_settings mark="false"/>
        </presentation_settings>
    </presentation>
</interface>
