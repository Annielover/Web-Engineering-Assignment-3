<Name>name</Name>
<TemporalOperator name="SequentialEnablingInfo"/>
<Parent name="Reply to attention"/>
<SiblingRight name="Confirmation"/>
                            </Task>
                            <Task Identifier="Confirmation" Category="application" Iterative="false" Optional="false" PartOfCooperation="false" Frequency=" ">
<Name>name</Name>
<TemporalOperator name="SequentialEnablingInfo"/>
<Parent name="Reply to attention"/>
<SiblingLeft name="choose attention"/>
<SiblingRight name="Confirm attention"/>
                            </Task>
                            <Task Identifier="Confirm attention" Category="interaction" Iterative="false" Optional="false" PartOfCooperation="false" Frequency=" ">
<Name>name</Name>
<TemporalOperator name="SequentialEnablingInfo"/>
<Parent name="Reply to attention"/>
<SiblingLeft name="Confirmation"/>
<SiblingRight name="Send attention"/>
                            </Task>
                            <Task Identifier="Send attention" Category="application" Iterative="false" Optional="false" PartOfCooperation="false" Frequency=" ">
<Name>name</Name>
<Parent name="Reply to attention"/>
<SiblingLeft name="Confirm attention"/>
                            </Task>
                        </SubTask>
                    </Task>
