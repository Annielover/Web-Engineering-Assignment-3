        <SubTask>
            <Task Identifier="Send attention" Category="application" Iterative="false" Optional="false" PartOfCooperation="false" Frequency=" ">
                <Name>name</Name>
                <TemporalOperator name="SequentialEnablingInfo"/>
                <Parent name="Notify for attention"/>
                <SiblingRight name="User receives attention"/>
            </Task>
            <Task Identifier="User receives attention" Category="application" Iterative="false" Optional="false" PartOfCooperation="false" Frequency=" ">
                <Name>name</Name>
                <TemporalOperator name="SequentialEnablingInfo"/>
                <Parent name="Notify for attention"/>
