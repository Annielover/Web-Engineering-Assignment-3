<TaskModel xmlns="http://giove.isti.cnr.it/ctt" xmlns:coop="http://giove.isti.cnr.it/cttcoop" NameTaskModelID="Notify for attention">
    <Task Identifier="Notify for attention" Category="abstraction" Iterative="false" Optional="false" PartOfCooperation="false" Frequency=" ">
